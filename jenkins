pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage("build"){
            steps{
                echo "project build"
            }
        }
        stage("test"){
            steps{
                echo  "project test"
            }
        }
        stage("deploy"){
            steps{
                echo "project deploy"
            }
        }
        
    }
    
}
